{
  "name": "snaphaven",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:local": "env-cmd -f .env.local next dev -p 3000 -H 0.0.0.0",
    "start:dev": "env-cmd -f .env.dev next start -p 3000 -H 0.0.0.0",
    "build:dev": "env-cmd -f .env.dev next build",
    "start:prod": "env-cmd -f .env.prod next start -p 3000",
    "build:prod": "env-cmd -f .env.prod next build",
    "docker:build:dev": "docker build --build-arg TARGET_ENV=dev -t snaphaven-client-dev .",
    "docker:run:dev": "docker run -d -p 3000:3000 --name snaphaven-client-dev --env-file .env.dev snaphaven-client-dev",
    "docker:build:prod": "docker build --build-arg TARGET_ENV=prod -t snaphaven-client-prod .",
    "docker:run:prod": "docker run -d -p 3000:3000 --name snaphaven-client-prod --env-file .env.prod snaphaven-client-prod",
    "docker:stop": "docker stop snaphaven-client-dev snaphaven-client-prod 2>/dev/null && docker rm snaphaven-client-dev snaphaven-client-prod 2>/dev/null",
    "start": "if [ \"$NODE_ENV\" = \"prod\" ]; then npm run start:prod; else npm run start:dev; fi",
    "run": "yarn build && yarn start",
    "lint": "next lint"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.820.0",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-slot": "^1.1.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "crypto": "^1.0.1",
    "env-cmd": "^11.0.0",
    "eslint-config-prettier": "^10.0.1",
    "file-saver": "^2.0.5",
    "form-data": "^4.0.5",
    "formidable": "^3.5.4",
    "fs": "^0.0.1-security",
    "jose": "^6.0.11",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.475.0",
    "mongodb": "^6.12.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.13.0",
    "next": "^15.3.1",
    "next-auth": "^4.24.11",
    "next-connect": "^1.0.0",
    "next-themes": "^0.4.4",
    "nodemailer": "^7.0.10",
    "raw-body": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.66.1",
    "react-photo-album": "^3.0.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/file-saver": "^2.0.7",
    "@types/form-data": "^2.2.1",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/jszip": "^3.4.0",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^1.4.12",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "description": "This is a [Next.js](https://nextjs.org) project bootstrapped with [`create-next-app`](https://nextjs.org/docs/app/api-reference/cli/create-next-app).",
  "main": "index.js",
  "author": "",
  "license": "ISC"
}
